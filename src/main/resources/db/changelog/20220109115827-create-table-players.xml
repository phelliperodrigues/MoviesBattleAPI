<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1641740251435-4" author="Phellipe Rodrigues">
        <createTable tableName="players">
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_players"/>
            </column>
            <column name="nickname" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="user_code" type="BIGINT"/>
            <column name="playing" type="BOOLEAN">
                <constraints nullable="false"/>
            </column>
            <column name="score" type="DOUBLE"/>
        </createTable>
    </changeSet>

</databaseChangeLog>